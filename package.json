    "@types/nodemailer": "^6.4.16",
    "adm-zip": "^0.5.16",
    "clipboardy": "^4.0.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "experimental-allure-playwright": "0.0.3",
    "globals": "^15.9.0",
    "jasmine": "5.3.0",
    "jasmine-allure-reporter": "^1.0.2",
    "jest": "29.7.0",
    "jest-allure": "^0.1.3",
    "jest-playwright-preset": "4.0.0",
    "lighthouse": "^9.3.1",
    "nodemailer": "^6.9.15",
    "playwright-lighthouse": "^2.2.2",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2",
    "typescript": "^5.6.2",
    "winston": "^3.14.2",
    "xlsx": "^0.18.5"
