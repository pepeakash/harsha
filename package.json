{
  "name": "playwright",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx playwright install",
    "report": "npx playwright show-report",
    "testjest": "jest",
    "test": "npx playwright test",
    "testEnvironment": "cross-env test_env=test npx playwright test --project=chromium",
    "prodEnvironment": "cross-env test_env=prod npx playwright test --project=chromium",
    "devEnvironment": "cross-env test_env=development npx playwright test --project=chromium",
    "chromium": "npx playwright test --project=chromium",
    "update-snapshots": "npx playwright test --update-snapshots",
    "debug": "npx playwright test --project=chromium --debug",
    "chrome": "npx playwright test --project=chrome -x",
    "webTests": "npx playwright test --grep @Web",
    "APITests": "npx playwright test --grep @API",
    "SafariNewConfig": "npx playwright test --config playwright.config1.js --project=safari",
    "prettier": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "authStorage": "npx playwright codegen --save-storage=auth.json",
    "authOpen": "npx playwright open --load-storage=auth.json instagram.com",
    "envStaging": "cross-env ENVIRONMENT=staging npx playwright test"
  },

  "keywords": [],
  "author": "",
  "license": "ISC",
